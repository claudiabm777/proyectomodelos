# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                               controllers.HomeController.index
GET     /formAsignacionTrabajadores     controllers.HomeController.formprogdina
GET     /formAsignTrabCompleto          controllers.HomeController.formprogdinalleno
GET     /formBuenaOpcion                controllers.HomeController.formalgoritmo
GET     /formBuenaOpcionCompleto        controllers.HomeController.formalgoritmolleno
GET     /formIndicadores                controllers.HomeController.formredjackson
GET     /formIndicadoresCompleto        controllers.HomeController.formredjacksonlleno

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
